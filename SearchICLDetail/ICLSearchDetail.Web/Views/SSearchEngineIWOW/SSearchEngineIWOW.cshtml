@{ ViewBag.Title = "Search Inward/Outward Check Details"; }
<script src="~/ScriptsNg/Controller/IWOW/searchEngineIWOW.js"></script>
<head>
    <link rel="stylesheet" href="~/ScriptsNg/styles.css" type="text/css">
</head>
<body onload="document.getElementById('btnInward').click(); ">
    <form name="yourId" id="yourId" class="form" ng-controller="searchEngineIWOW" ng-app="indexApp" autocomplete="off">

        <!--     <div class="header">
        </div>-->
        <br />
        <br />
        <br />

        <div class="header">
            <h1>SEARCH ENGINE - IW and OW Details</h1>
            <p><i>Search engine that will display inward and outward details corresponds to user search criteria.</i></p>
        </div>
        <button type="button" class="tablink active" ng-focus="myFunction()" ng-click="openPage('Inward', 'btnInward', '#800000')" id="btnInward" style="">Inward</button>
        <button type="button" class="tablink2" ng-focus="myFunction()" ng-click="openPage('Outward', 'btnOutward', '#800000')" id="btnOutward" style="">Outward</button>
        <br /><br />

        @*inward tab*@
        <div id="Inward" class="tabcontent">
            <div class="sidebar" id="sidebar" style="border:thin;">
                <table>
                    <tr>
                        <td style="padding-right:15px">
                            <label for="radioSearchUserID">
                                <input type="radio" name="radioUserID" id="radioSearchUserID" ng-model="radioUserID" ng-click="radioSearchBatchClick()" checked="isChecked">
                            </label>
                        </td>
                        <td></td>
                        <td><b>By User and Verification Date(time)</b></td>
                    </tr>
                </table>
                <div>
                    <div class="radio">
                        <fieldset>
                            <table border="0" class="modalTr">
                                <tr>
                                    <td col title="mandatory field">
                                        User ID<span style="font-size:18px;color:red"><sup>*</sup></span>

                                    </td>
                                    <td>
                                        <!-- <span style="font-size:9px;color:red">*REQUIRED </span> -->
                                        <input type="text" title="accepts alphanumeric char(s) only" name="txtUserId" id="txtUserId" ng-model="txtUserId" ng-disabled="radioDisabled2" class="form-control" style="border-left-width:2px; background-color:powderblue" @*style="width:100%;float:right; background-color: #b5f5c6;color: black;"*@ placeholder="Enter User ID" ng-focus="searchByFocus(1)" ng-required="isRequired2" ng-change="ngChangeRequired(txtUserId)" />
                                    </td>
                                </tr>
                                <tr>
                                    <td title="mandatory field">
                                        Verification Date<span style="font-size:18px;color:red"><sup>*</sup></span>
                                    </td>
                                    <td>
                                        <!--<span style="font-size:9px;color:red"> *REQUIRED </span>-->
                                        <input id="input-date" name="input-date" type="date" ng-disabled="radioDisabled2" class="form-control" placeholder="mm-dd-yyyy" style="border-left-width:2px; background-color:powderblue" ng-focus="searchByFocus(1)" max="9999-12-31" ng-required="isRequired2" />
                                    </td>

                                </tr>
                                <tr>
                                    <td>
                                        Verification Time
                                    </td>
                                    <td>
                                        <input id="input-time" name="input-time" type="time" ng-disabled="radioDisabled2" class="form-control" ng-focus="searchByFocus(1)" style="width:100%;float:right;" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-size:10px;color:red">*REQUIRED fields</span>
                                    </td>
                                </tr>
                            </table>
                        </fieldset>

                    </div>
                </div>
                <br />
                <table>
                    <tr>
                        <td style="padding-right:15px">
                            <label for="radioSearchCheckNo">
                                <input type="radio" name="radioCheckNo" id="radioSearchCheckNo" ng-model="radioCheckNo" ng-click="radioSearchBatchClick1()">
                            </label>
                        </td>

                        <td><b>By Check Number</b></td>
                    </tr>
                </table>
                <div class="radio">
                    <fieldset>
                        <table @*style="width:100%"*@ border="0" class="modalTr">

                            <tr>
                                <td title="mandatory field">
                                    Check Number<span style="font-size:18px;color:red"><sup>*</sup></span>
                                </td>
                                <td>
                                    <input type="text" title="accepts numeric char(s) only" name="txtCheckNumber" id="txtCheckNumber" ng-disabled="radioDisabled1" class="form-control" style="border-left-width:2px; background-color:powderblue" @*style="width:100%;float:right; background-color: #b5f5c6;color: black;"*@ placeholder="Enter Check Number" maxlength="10" ng-required="isRequired2" />
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    User ID
                                </td>
                                <td>
                                    <!-- <span style="font-size:9px"> accepts alphanumeric char(s) only</span> -->
